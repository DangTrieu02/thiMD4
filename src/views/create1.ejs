<body>
<form action="/create"
      method="post">
    <div class="form-group">
        <label for="name">Tên</label>
        <input type="text"  class="form-control" id="studentname" name="studentname" multiple />

    </div>
    <div class="form-group">
        <label for="studentid">id</label>
        <input type="text"  class="form-control" id="studentid" name="studentid" multiple />
    </div>
    <div class="form-group">
        <label for="name">điểm lý thuyết</label>
        <input type="text"  class="form-control" id="scorelt" name="scorelt   " multiple />
    </div>
    <div class="form-group">
        <label for="name">Điểm thực hành</label>
        <input type="text"  class="form-control" id="scoreth" name="scoreth" multiple />
    </div>
    <div class="form-group">
        <label for="name">mô tả</label>
        <input type="text"  class="form-control" id="access" name="access" multiple />
    </div>
    <div class="form-group">
        <label for="price">đánh giá</label>
        <input type="text"  class="form-control" id="description" name="description" />
    </div>
    <div class="form-group">
        <label for="quantity">lớp</label>
        <input type="text" class="form-control" id="class" name="class" />
    </div>



    <button type="submit" class="btn btn-primary">Submit</button>
</form>




















